<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TabsView">
        <attr name="normalColor" format="color"/>
        <attr name="checkedColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="RippleView">
        <attr name="rv_alpha" format="integer" />
        <attr name="rv_framerate" format="integer"/>
        <attr name="rv_rippleDuration" format="integer"/>
        <attr name="rv_zoomDuration" format="integer" />
        <attr name="rv_color" format="color" />
        <attr name="rv_centered" format="boolean" />
        <attr name="rv_type" format="enum">
            <enum name="simpleRipple" value="0"/>
            <enum name="doubleRipple" value="1"/>
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="rv_ripplePadding" format="dimension" />
        <attr name="rv_zoom" format="boolean" />
        <attr name="rv_zoomScale" format="float" />
    </declare-styleable>

    <declare-styleable name="CompoundFontIconImageView">
        <attr name="cfiiy_iconSize" format="dimension|reference" />

        <attr name="cfiiv_icon" format="enum">
            <enum name="filter" value="1001"/>
            <enum name="close" value="1002"/>
            <enum name="image" value="1003"/>
            <enum name="tab_home" value="1004"/>
            <enum name="tab_home_selected" value="1005"/>
            <enum name="tab_game" value="1006"/>
            <enum name="tab_game_selected" value="1007"/>
            <enum name="tab_software" value="1008"/>
            <enum name="tab_software_selected" value="1009"/>
            <enum name="tab_group" value="1010"/>
            <enum name="tab_group_selected" value="1011"/>
            <enum name="tab_manager" value="1012"/>
            <enum name="tab_manager_selected" value="1013"/>
        </attr>
        <attr name="cfiiy_iconColor" format="color|reference" />

        <attr name="cfiiv_checked_icon" format="enum">
            <enum name="filter" value="1001"/>
            <enum name="close" value="1002"/>
            <enum name="image" value="1003"/>
            <enum name="tab_home" value="1004"/>
            <enum name="tab_home_selected" value="1005"/>
            <enum name="tab_game" value="1006"/>
            <enum name="tab_game_selected" value="1007"/>
            <enum name="tab_software" value="1008"/>
            <enum name="tab_software_selected" value="1009"/>
            <enum name="tab_group" value="1010"/>
            <enum name="tab_group_selected" value="1011"/>
            <enum name="tab_manager" value="1012"/>
            <enum name="tab_manager_selected" value="1013"/>
        </attr>
        <attr name="cfiiy_checked_iconColor" format="color|reference" />

        <attr name="cfiiy_checked" format="boolean"/>
    </declare-styleable>
</resources>